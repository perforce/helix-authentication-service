{
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "unicorn"
  ],
  "extends": "eslint:recommended",
  "rules": {
    "unicorn/prefer-module": "error",
    "unicorn/prefer-node-protocol": "warn"
  }
}
